{  "preset": "laravel",  "rules": {    "binary_operator_spaces": {      "default": "align_single_space_minimal"    },    "array_syntax": { "syntax": "short" },    "no_unused_imports": true,    "not_operator_with_space": false,    "trailing_comma_in_multiline": { "after_heredoc": true },    "align_multiline_comment": true,    "phpdoc_align": true  }}